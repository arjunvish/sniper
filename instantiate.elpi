pred instantiate i: term, i: term, o: term.
instantiate (prod N Ty F) T (app [(prod N Ty F), T]) :- coq.typecheck T TyT ok, coq.unify-leq TyT Ty ok. 
instantiate T _ T. 

pred instantiate_list i: term, i: (list term), o: (list term).
instantiate_list T [U|L] [V|M] :- instantiate T U V, 
instantiate_list T L M. 
instantiate_list T [] [].
